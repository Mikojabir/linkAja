							"            \"id\": 7,\r",
							"            \"email\": \"michael.lawson@reqres.in\",\r",
							"            \"first_name\": \"Michael\",\r",
							"            \"last_name\": \"Lawson\",\r",
							"            \"avatar\": \"https://reqres.in/img/faces/7-image.jpg\"\r",
							"        };\r",
							"\r",
