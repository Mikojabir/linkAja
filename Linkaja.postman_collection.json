					"listen": "test",
					"script": {
						"exec": [
							"var body = pm.response.json();\r",
							"\r",
							"/**\r",
							" * Post - User login\r",
							" */\r",
							"pm.test('TC1 - User login success', function () {\r",
							"\r",
							"    //validation response code \r",
							"    pm.response.to.have.status(200);\r",
							"\r",
							"    //validation response body type\r",
							"    pm.expect(body.id).to.be.a('number');\r",
							"    pm.expect(body.token).to.be.a('string');\r",
							"\r",
							"    //validation response body key - value\r",
							"    var expectObj = {\r",
							"        \"id\": 1,\r",
							"        \"token\": \"QpwL5tke4Pnpja7X1\"\r",
							"    }\r",
							"\r",
							"    pm.expect(body).to.include(expectObj);\r",
							"\r",
							"});\r",
							"\r",
							"/**\r",
							" * Negative case - Email not registered\r",
							" */\r",
							"pm.test('TC2 - Email not registered', function () {\r",
							"\r",
							"    let baseURL = 'https://reqres.in'\r",
							"    let getTokenRequest = {\r",
							"        url: baseURL+'/api/register',\r",
							"        method: 'POST',\r",
							"        header: 'Content-Type:application/json',\r",
							"        body: {\r",
							"            raw: JSON.stringify(\r",
							"                {\r",
							"                    email: \"george.bluth@gmail.com\",\r",
							"                    password: \"George Bluth\"\r",
							"                })\r",
							"        }\r",
							"    }\r",
							"    pm.sendRequest(getTokenRequest, (error, response) => {\r",
							"\r",
							"        let body = response.json()\r",
							"        let respCode = response.code\r",
							"\r",
							"        //response code\r",
							"        if (respCode == 400) {\r",
							"            console.info(\"TC2 - Response Code: \" + respCode)\r",
							"        } else {\r",
							"            console.error(error + \"TC2 Response Code Not Passed\" + respCode)\r",
							"        }\r",
							"\r",
							"        //validation body response\r",
							"        pm.expect(body.error).equal('Missing email or username');\r",
							"\r",
							"    });\r",
							"});\r",
