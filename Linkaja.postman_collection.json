		{
			"name": "Get User",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var body = pm.response.json();\r",
							"\r",
							"/**\r",
							" * Get Users \r",
							" */\r",
							"pm.test('TC1 - Get users success', function () {\r",
							"\r",
							"    //validation response code \r",
							"    pm.response.to.have.status(200);\r",
							"\r",
							"    //validation response body type\r",
							"    pm.expect(body.data).to.be.an('array');\r",
							"    pm.expect(body.data[0].id).to.be.a('number');\r",
							"    pm.expect(body.data[0].email).to.be.a('string');\r",
							"    pm.expect(body.data[0].first_name).to.be.a('string');\r",
							"    pm.expect(body.data[0].last_name).to.be.a('string');\r",
							"    pm.expect(body.data[0].avatar).to.be.a('string');\r",
							"\r",
							"    //validation response body key - value\r",
							"    var expectObj = {\r",
							"        \"id\": 1,\r",
							"        \"email\": \"george.bluth@reqres.in\",\r",
							"        \"first_name\": \"George\",\r",
							"        \"last_name\": \"Bluth\",\r",
							"        \"avatar\": \"https://reqres.in/img/faces/1-image.jpg\"\r",
							"    };\r",
							"\r",
							"    pm.expect(body.data[0]).to.include(expectObj);\r",
							"\r",
							"});\r",
							"\r",
							"/**\r",
							" * Get Users\r",
							" * query param 'id'\r",
							" */\r",
							"pm.test('TC2 - Filter by id with query param', function () {\r",
							"    let baseURL = 'https://reqres.in'\r",
							"    let endpoint = '/api/users?id=1'\r",
							"    let setting = {\r",
							"        url: baseURL + endpoint,\r",
							"        method: 'GET',\r",
							"        header: {\r",
							"            \"Content-Type\": \"application/json\"\r",
							"        }\r",
							"    }\r",
							"    pm.sendRequest(setting, (err, response) => {\r",
							"\r",
							"        let respCode = response.code\r",
							"        let body = response.json()\r",
							"\r",
							"        //response code\r",
							"        if (respCode == 200) {\r",
							"            console.info(\"TC2 - Response Code: \" + respCode)\r",
							"        } else {\r",
							"            console.error(\"Error, Response Code: \" + respCode)\r",
							"        }\r",
							"\r",
							"        //validation response body\r",
							"        pm.expect(body.data.id).equal(1);\r",
							"        pm.expect(body.data.email).to.include('reqres');\r",
							"        pm.expect(body.data.first_name).equal('George');\r",
							"        pm.expect(body.data.last_name).equal('Bluth');\r",
							"        pm.expect(body.data.avatar).to.include('img')\r",
							"\r",
							"\r",
							"    });\r",
							"\r",
							"});\r",
							"\r",
